{
  "srl_mask": {
    "penalty": 1.0,
    "viterbi": false,
    "token_take_mask": {
      "layer": "joint_pos_predicate",
      "output": "predicate_predictions"
    },
    "output_fn": {
      "name": "srl_arg_detection",
